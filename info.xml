<?xml version="1.0"?>
<extension key="com.skvare.redis_healthcheck" type="module">
  <file>redis_healthcheck</file>
  <name>Redis Health Check</name>
  <description>Comprehensive Redis cache monitoring and diagnostics for CiviCRM. Provides detailed health checks, performance metrics, memory usage analysis, and configuration verification through the System Status Check interface.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Sunil Pawar</author>
    <email>sunil@skvare.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/Skvare/com.skvare.redis_healthcheck</url>
    <url desc="Documentation">https://github.com/Skvare/com.skvare.redis_healthcheck</url>
    <url desc="Support">https://github.com/Skvare/com.skvare.redis_healthcheck</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2026-02-11</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>6.10</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/RedisHealthcheck</namespace>
    <format>23.02.1</format>
    <angularModule>crmRedisHealthcheck</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
